{
  "main": ["handsontable.full"],
  "files": [
      "dist/handsontable.full.js",
      "dist/handsontable.full.css",
      "plugins/*",
      "src/*"
     ],
  "format": "cjs",
  "shim": {
    "handsontable.full": {
      "deps": ["zeroclipboard","moment","pikaday","jquery"],
      "exports": "Handsontable"
    }
  },
  "dependencies": {
    "zeroclipboard": "npm:zeroclipboard@^2.1.6",
    "pikaday": "npm:pikaday@^1.3.2",
    "moment": "npm:moment@^2.2.0"
  }

}
